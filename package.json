{
  "name": "react-notification-badge",
  "version": "1.3.0",
  "description": "Simple notification badge react component",
  "main": "lib/index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test:local": "karma start",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-notification-badge"
  },
  "keywords": [
    "react",
    "react-component",
    "badge",
    "notification"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-notification-badge/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-notification-badge",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.12.3",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "karma": "^1.2.0",
    "karma-browserify": "^5.0.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
